{
  "manifest_version": 2,
  "name": "Finn.no - external map",
  "version": "2.0",
  "description": "Allows for address lookup on kart.finn.no",
  "icons": {
    "48": "icons/icon-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.finn.no/*?finnkode=*"],
      "js": ["mapViewer.js"]
    }
  ]
}
